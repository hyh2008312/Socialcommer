{
  "name": "SocialCommer",
  "version": "1.1.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 8080",
    "start:ssr": "ng build --dev --aot=true --app 3 && ng build --dev --aot=true --app 4",
    "start:server": "webpack --progress --env.development --colors",
    "ssr": "npm run build:universal && npm run server",
    "ssr:debug": "ng build --dev --aot=true --output-hashing=all --named-chunks=false --build-optimizer=true --app 1 && ng build --dev --aot=true --output-hashing=all --named-chunks=false --build-optimizer=true --app 2 && webpack --env.development && node server.js",
    "ssr:cw": "ng build --dev --aot=true --output-hashing=all --named-chunks=false --build-optimizer=true --app 1 --watch",
    "ssr:sw": "ng build --dev --aot=true --output-hashing=all --named-chunks=false --build-optimizer=true --app 2 --watch",
    "ssr:webpack": "webpack --watch",
    "ssr:server": "nodemon dist/server.js",
    "build": "ng build",
    "build:prod": "ng build --prod --app 1",
    "build:dev": "ng build --prod -e dev --app 5",
    "build:server": "ng build --prod --app 2",
    "build:universal": "ng build --prod --app 1 && ng build --prod --app 2 && webpack --env.production --progress --colors",
    "build:universalServer": "webpack --env.production --progress --colors",
    "server": "node dist/server.js",
    "server:csr": "node csr_server.js",
    "server:test": "node test-dist/server.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "universal": "ng build --prod --app 1 && ng build --prod --app 2 && ts-node server",
    "build:prerender": "npm run build:universal && npm run generate:prerender",
    "generate:prerender": "node prerender.js",
    "generate:test": "node test-dist/prerender.js"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/schematics": "0.0.35",
    "@angular/animations": "^5.2.10",
    "@angular/cdk": "^5.2.5",
    "@angular/common": "^5.2.10",
    "@angular/compiler": "^5.2.10",
    "@angular/core": "^5.2.10",
    "@angular/flex-layout": "^5.0.0-beta.13",
    "@angular/forms": "^5.2.10",
    "@angular/http": "^5.2.10",
    "@angular/material": "^5.2.5",
    "@angular/platform-browser": "^5.2.10",
    "@angular/platform-browser-dynamic": "^5.2.10",
    "@angular/platform-server": "^5.2.10",
    "@angular/router": "^5.2.10",
    "@nguniversal/common": "^5.0.0",
    "@nguniversal/express-engine": "^5.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^5.0.0",
    "@ngx-meta/core": "^5.0.0",
    "@ngx-translate/core": "^9.1.1",
    "@ngx-translate/http-loader": "^2.0.1",
    "ajv": "^5.5.2",
    "ajv-keywords": "^2.1.1",
    "angular-cropperjs": "^0.1.5",
    "angular2-moment": "^1.8.0",
    "angular2-social-login": "^3.1.1",
    "angular4-carousel": "^3.1.8",
    "angulartics2": "^4.6.3",
    "body-parser": "^1.18.2",
    "classlist.js": "^1.1.20150312",
    "compression": "^1.7.2",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.5.5",
    "enhanced-resolve": "^3.4.1",
    "express": "^4.16.3",
    "intl": "^1.2.5",
    "mongodb": "^2.2.35",
    "morgan": "^1.9.0",
    "ngx-auth": "^3.1.0",
    "ngx-clipboard": "^10.0.0",
    "ngx-cookie-service": "^1.0.10",
    "ngx-quill-editor": "^2.2.2",
    "ngx-swiper-wrapper": "^5.3.8",
    "node-fetch": "^2.1.2",
    "prerender-node": "^2.8.0",
    "rxjs": "^5.5.10",
    "throng": "^4.0.0",
    "ts-loader": "^3.5.0",
    "typescript": "^2.8.3",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.4",
    "@angular/compiler-cli": "^5.0.0",
    "@types/jasmine": "2.8.2",
    "@types/node": "~8.0.52",
    "codelyzer": "~4.0.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "localstorage-polyfill": "^1.0.1",
    "node-sass": "^4.7.2",
    "nodemon": "^1.17.2",
    "preboot": "^6.0.0-beta.3",
    "protractor": "~5.2.0",
    "ts-node": "~3.3.0",
    "tslint": "~5.8.0",
    "typescript": "^2.5.2"
  }
}
