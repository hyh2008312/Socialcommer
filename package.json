    "enhanced-resolve": "^3.3.0",
    "express": "^4.15.3",
    "flex-layout": "^1.0.1",
    "intl": "^1.2.5",
    "mongodb": "^2.2.30",
    "morgan": "^1.8.2",
    "ngx-auth": "^3.0.0",
    "ngx-cookie-service": "^1.0.10",
