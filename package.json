{
  "name": "SocialCommer",
  "version": "1.1.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 8080",
    "ssr": "npm run build:universal && npm run server",
    "ssr:debug": "ng build --dev --aot=true --output-hashing=all --named-chunks=false --build-optimizer=true  && ng build --dev --aot=true --output-hashing=all --named-chunks=false --build-optimizer=true --app 1 && webpack && node server.js",
    "ssr:cw": "ng build --dev --aot=true --output-hashing=all --named-chunks=false --build-optimizer=true --watch",
    "ssr:sw": "ng build --dev --aot=true --output-hashing=all --named-chunks=false --build-optimizer=true --app 1 --watch",
    "ssr:webpack": "webpack --watch",
    "ssr:server": "nodemon dist/server.js",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "build:universal": "ng build --prod && ng build --prod --app 1 && webpack --progress --colors",
    "server": "node dist/server.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "universal": "ng build --prod && ng build --prod --app 1 && ts-node server",
    "build:prerender": "npm run build:universal && npm run generate:prerender",
    "generate:prerender": "node prerender.js"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/schematics": "0.0.35",
    "@angular/animations": "^5.0.1",
    "@angular/cdk": "^5.0.0-rc0",
